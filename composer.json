{
    "name": "tfohrer/mafeah",
    "type": "wordpress-theme",
    "description": "Custom wordpress theme for mafeah based on Jackpine.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tarek Fohrer",
            "email": "t.fohrer@gmx.de"
        }
    ],
    "require": {
        "php": "^7.3",
        "timber/timber": "dev-2.x-term-factories",
        "wpackio/enqueue": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Mafeah\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
